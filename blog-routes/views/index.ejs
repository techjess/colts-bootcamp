<% include ./partials/header%>

<h1>This is the Blogs Post</h1>


<% blogs.forEach(function(blog){ %>
    <div >
      <h2><%= blog.title%></h2>
      <img src="<%= blog.image%>" alt="">
      <span><%= blog.created%></span>
      <p><%= blog.body%></p>
    </div>
<%}) %>


<% include ./partials/footer %>
